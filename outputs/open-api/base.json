{
  "additionalProperties": false,
  "properties": {
    "avatar": {
      "format": "uri",
      "type": "string"
    },
    "birthTime": {
      "type": "integer"
    },
    "birthday": {
      "format": "date-time",
      "type": "string"
    },
    "certificate": {
      "format": "binary",
      "type": "string"
    },
    "dynamicKeyHolder": {
      "additionalProperties": false,
      "description": "Some kind of list",
      "properties": {
        "/s/": {
          "additionalProperties": false,
          "properties": {
            "id": {
              "description": "Tbe ID for the reference",
              "example": 123,
              "type": "number"
            },
            "name": {
              "description": "Name of something",
              "example": "Jack",
              "nullable": true,
              "type": "string"
            }
          },
          "required": [
            "id",
            "name"
          ],
          "type": "object"
        }
      },
      "type": "object"
    },
    "email": {
      "format": "email",
      "type": "string"
    },
    "facebookId": {
      "nullable": true,
      "type": "string"
    },
    "type": {
      "enum": ["user"],
      "type": "string"
    },
    "gender": {
      "default": "Male",
      "enum": [
        "Male",
        "Female",
        "",
        null
      ],
      "nullable": true,
      "type": "string"
    },
    "genderSpecific": {
      "oneOf": [
        {
          "type": "number",
          "enum": [
            0,
            1,
            2
          ]
        },
        {
          "type": "string"
        }
      ]
    },
    "guid": {
      "format": "uuid",
      "type": "string"
    },
    "height": {
      "exclusiveMaximum": true,
      "exclusiveMinimum": true,
      "maximum": 200,
      "minimum": 0,
      "type": "number"
    },
    "hostname": {
      "format": "hostname",
      "type": "string"
    },
    "ip": {
      "oneOf": [
        {
          "format": "ipv4"
        },
        {
          "format": "ipv6"
        }
      ],
      "type": "string"
    },
    "isoDateString": {
      "format": "date-time",
      "type": "string"
    },
    "maleSpecific": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "type": [
            "array",
            "boolean",
            "number",
            "object",
            "string",
            "null"
          ]
        }
      ]
    },
    "meta": {
      "additionalProperties": false,
      "properties": {
        "hash": {
          "format": "override_ip_format",
          "type": "string"
        },
        "optional": {
          "format": "custom_format",
          "type": "number"
        }
      },
      "required": [
        "hash"
      ],
      "type": "object"
    },
    "nested": {
      "additionalProperties": true,
      "properties": {
        "key": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "nickName": {
      "description": "Hero Nickname",
      "example": "鹄思乱想",
      "maxLength": 20,
      "minLength": 3,
      "pattern": "^[a-z]+$",
      "type": "string"
    },
    "notes": {
      "type": [
        "array",
        "boolean",
        "number",
        "object",
        "string",
        "null"
      ],
      "x-supported-lang": [
        "zh-CN",
        "en-US"
      ]
    },
    "password": {
      "not": {}
    },
    "retired": {
      "type": "boolean"
    },
    "skills": {
      "description": "Skills",
      "items": {
        "oneOf": [
          {
            "type": "string"
          },
          {
            "additionalProperties": true,
            "properties": {
              "level": {
                "default": 50,
                "description": "Skill Level",
                "example": 10,
                "maximum": 100,
                "minimum": 10,
                "multipleOf": 10,
                "type": "integer"
              },
              "name": {
                "description": "Skill Name",
                "example": "teleport",
                "type": "string"
              }
            },
            "required": [
              "name"
            ],
            "type": "object",
            "x-expandable": true,
            "x-lookup": "name"
          }
        ]
      },
      "maxItems": 3,
      "minItems": 1,
      "type": "array",
      "uniqueItems": true,
      "x-exchangable": false
    },
    "tags": {
      "items": {
        "type": "string"
      },
      "maxItems": 2,
      "minItems": 2,
      "type": "array"
    },
    "uuid": {
      "format": "uuid",
      "type": "string"
    }
  },
  "required": [
    "nickName",
    "avatar",
    "meta"
  ],
  "type": "object"
}
